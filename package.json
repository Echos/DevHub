    "serve-favicon": "^2.0.1",
    "socket.io": "1.4.5",
    "underscore": "^1.7.0",
    "jquery": "2.2.2",
    "jquery-ui": "1.10.5",
    "jquery-colorbox": "1.6.3",
    "jquery-lazyload": "1.9.7",
    "bootstrap": "3.3.6",
    "favico.js": "0.3.10",
    "jquery-autosize": "1.18.18",
    "knockout": "3.4.0",
