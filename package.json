    "ejs": "2.4.1",
    "express": "4.13.4",
    "gridfs-stream": "^1.1.1",
    "grunt": "~0.4.2",
    "grunt-contrib-cssmin": "~1.0.0",
    "grunt-contrib-uglify": "~1.0.0",
    "grunt-contrib-watch": "~1.0.0",
    "moment": "2.12.0",
